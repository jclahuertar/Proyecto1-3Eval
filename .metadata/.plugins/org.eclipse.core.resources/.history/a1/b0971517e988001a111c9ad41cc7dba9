import java.util.Vector;

public class FamiliarS extends Persona {
	
//	FAMILIAR 1
//	
//	telefonoFamiliar1
//	emailFamiliar1

	private String dni_nie;
	private Boolean autorizadoEnSigad;
	private Boolean conviveConAlumno;
	private int telfnoUno;
	private int telfnoDos;
	private String email;

	/**
	 * 
	 * @element-type Alumno
	 */
	public Vector  FamiliarAlumno;

	public FamiliarS() {
	}

	public String getDni_nie() {
		return dni_nie;
	}

	public void setDni_nie(String dni_nie) {
		this.dni_nie = dni_nie;
	}

	public Boolean getAutorizadoEnSigad() {
		return autorizadoEnSigad;
	}

	public void setAutorizadoEnSigad(Boolean autorizadoEnSigad) {
		this.autorizadoEnSigad = autorizadoEnSigad;
	}

	public Boolean getConviveConAlumno() {
		return conviveConAlumno;
	}

	public void setConviveConAlumno(Boolean conviveConAlumno) {
		this.conviveConAlumno = conviveConAlumno;
	}

	public int getTelfnoUno() {
		return telfnoUno;
	}

	public void setTelfnoUno(int telfnoUno) {
		this.telfnoUno = telfnoUno;
	}

	public int getTelfnoDos() {
		return telfnoDos;
	}

	public void setTelfnoDos(int telfnoDos) {
		this.telfnoDos = telfnoDos;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public Vector getFamiliarAlumno() {
		return FamiliarAlumno;
	}

	public void setFamiliarAlumno(Vector familiarAlumno) {
		FamiliarAlumno = familiarAlumno;
	}

	@Override
	public String toString() {
		return super.toString() + "Familiar dni_nie= " + dni_nie + " autorizadoEnSigad= " + autorizadoEnSigad + " conviveConAlumno= "
				+ conviveConAlumno + " telfnoUno= " + telfnoUno + " telfnoDos= " + telfnoDos + " email= " + email
				+ " FamiliarAlumno= " + FamiliarAlumno;
	}


}